{
  "name": "ga-anomalies-monitor-client",
  "version": "1.0.0",
  "description": "Client app for Google Analytics anomalies monitor",
  "main": "build/server.js",
  "engines": {
    "node": ">=5.0 <6",
    "npm": ">=3.3 <4"
  },
  "scripts": {
    "lint": "eslint src tools && jscs src tools",
    "csslint": "csscomb src/components --lint --verbose",
    "csscomb": "csscomb src/components --verbose",
    "test": "eslint src && jest",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "babel-node tools/run build",
    "deploy": "babel-node tools/run deploy",
    "dev": "babel-node tools/run start",
    "restart": "npm stop && npm start",
    "start": "npm run build -- --release && node ./node_modules/forever/bin/forever start build/server.js",
    "status": "node ./node_modules/forever/bin/forever list",
    "stop": "node ./node_modules/forever/bin/forever stop build/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fraszczakszymon/ga-anomalies-monitor-client"
  },
  "keywords": [
    "analytics",
    "monitor",
    "data"
  ],
  "author": "fraszczakszymon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fraszczakszymon/ga-anomalies-monitor-client/issues"
  },
  "homepage": "https://github.com/fraszczakszymon/ga-anomalies-monitor-client",
  "dependencies": {
    "Flot": "git+https://github.com/flot/flot.git#v0.8.3",
    "babel-core": "5.8.34",
    "bluebird": "3.1.1",
    "classnames": "2.2.1",
    "eventemitter3": "1.1.1",
    "express": "4.13.3",
    "fastclick": "1.0.6",
    "fbjs": "0.5.1",
    "forever": "^0.15.1",
    "front-matter": "2.0.1",
    "history": "1.17.0",
    "jade": "1.11.0",
    "jquery": "^2.2.0",
    "node-fetch": "1.3.3",
    "normalize.css": "3.0.3",
    "react": "0.14.3",
    "react-addons-css-transition-group": "^0.14.7",
    "react-dom": "0.14.3",
    "react-routing": "0.0.6",
    "source-map-support": "0.4.0",
    "whatwg-fetch": "0.10.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.2.0",
    "autoprefixer": "^6.1.2",
    "babel": "^5.8.34",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^5.4.0",
    "babel-plugin-react-transform": "^1.1.1",
    "browser-sync": "^2.10.1",
    "css-loader": "^0.23.0",
    "csscomb": "^3.1.8",
    "del": "^2.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.11.3",
    "file-loader": "^0.8.5",
    "gaze": "^0.5.2",
    "git-repository": "^0.1.1",
    "glob": "^6.0.1",
    "isomorphic-style-loader": "0.0.5",
    "jest-cli": "^0.8.2",
    "jscs": "^2.7.0",
    "json-loader": "^0.5.4",
    "lodash.merge": "^3.3.2",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "postcss": "^5.0.13",
    "postcss-import": "^7.1.3",
    "postcss-loader": "^0.8.0",
    "precss": "^1.3.0",
    "raw-loader": "^0.5.1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "replace": "^0.3.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-hot-middleware": "^2.6.0",
    "webpack-middleware": "^1.4.0"
  },
  "jest": {
    "rootDir": "./src",
    "scriptPreprocessor": "../preprocessor.js",
    "unmockedModulePathPatterns": [
      "fbjs",
      "react"
    ]
  }
}
